#include<stdio.h>
int main()
{
    int order_no;
    printf("\t1.Pizza, Rs 239/-\n\t2.Burger, Rs 129/-\n\t3.Pasta, Rs 179/-\n\t4.French Fries, Rs99/-\n\t5.Sandwich, Rs 149/-\n----------------------------------------------\n\tEnter your order no. :");
    scanf("%d", &order_no);
    switch(order_no)
    {
        case 1:
        printf("Food item - Pizza \n Price - Rs 239/-");break;
        case 2:
        printf("Food item - Burger \n Price - Rs 129/-");break;
        case 3:
        printf("Food item - Pasta \n Price - Rs 179/-");break;
        case 4:
        printf("Food item - French Fries \n Price - Rs 99/-");break;
        case 5:
        printf("Food item - Sandwich \n Price - Rs 149/-");break;
        default:
        printf("Sorry,..........Not Available.............");
    }
    return 0;
}
